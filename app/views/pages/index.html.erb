<% if current_user %>
  You are logged in as <%= current_user.email %><br>
  <h2>App: </h2>
  <%= link_to 'Applications', oauth_applications_path %><br>
  <h2>Post: </h2>
  <%= link_to 'Posts', posts_path %><br>

  <%= link_to 'Log Out', logout_path, method: :delete %>
<% else %>
  <%= link_to 'Log In', new_user_session_path %><br>
  <%= link_to 'Register', new_user_registration_path %>
<% end %>